<apex:page controller="ExpECM.ExpIntakeController" sidebar="false" tabStyle="ExpECM__Intake__tab" lightningStylesheets="true" >
<ExpECM:ExpPageVersion thePageVersion="13.0" theParentController="{!this}"/>
<head>
<apex:stylesheet value="{!URLFOR($Resource.ExpECM__ExpResources, styleSheet)}"/>
<ExpECM:ExpHeader />
</head>
<div class="ExpECMLEX">
<apex:form id="theForm">
<ExpECM:ExpFormTitle text="{!selectedProgram['Name']} Intake"/>
<apex:pageBlock id="thePageBlock">
<apex:pageMessages id="errors"/>
<ExpECM:ExpDuplicateList theController="{!baseController}"/>

<!-- **** Based off of ECM Configuration: PEEKE Intake Configuration ****-->

<!-- ***** Contact, page 1, order 1 ***** -->
<ExpECM:ExpContact theParentController="{!this}" csName="CompSettings-0408"/>

<!-- ***** Case Record, page 1, order 2 ***** -->
<ExpECM:ExpCaseRecord theParentController="{!this}" csName="CompSettings-0409"/>

<!-- ***** Case Record, page 1, order 3 ***** -->
<ExpECM:ExpCaseRecord theParentController="{!this}" csName="CompSettings-0410"/>

<!-- ***** Child Object List, page 1, order 4 ***** -->
<ExpECM:ExpChildObjectList theParentController="{!this}" csName="CompSettings-0411"/>

<!-- ***** Child Object List, page 1, order 5 ***** -->
<ExpECM:ExpChildObjectList theParentController="{!this}" csName="CompSettings-0412"/>

<apex:pageBlockButtons >
<ExpECM:ExpNavigationButton id="theButtons" theParentController="{!this}" thePageNum="1" postSaveRerender="theForm"/>
</apex:pageBlockButtons>
</apex:pageBlock>
</apex:form>
<apex:includeScript value="{!$Resource.ExpECM__ExpJavaScript}"/>
</div>
</apex:page>