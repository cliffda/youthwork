<apex:page controller="ExpECM.ExpIntakeController" sidebar="false" tabStyle="ExpECM__Intake__tab" lightningStylesheets="true" >
<ExpECM:ExpPageVersion thePageVersion="13.0" theParentController="{!this}"/>
<head>
<apex:stylesheet value="{!URLFOR($Resource.ExpECM__ExpResources, styleSheet)}"/>
<ExpECM:ExpHeader />
</head>
<div class="ExpECMLEX">
<apex:form id="theForm">
<ExpECM:ExpFormTitle text="{!selectedProgram['Name']} Intake"/>
<apex:pageBlock id="thePageBlock">
<apex:pageMessages id="errors"/>
<ExpECM:ExpDuplicateList theController="{!baseController}"/>

<!-- **** Based off of ECM Configuration: DOW Intake Configuration ****-->

<!-- ***** Contact, page 1, order 1 ***** -->
<ExpECM:ExpContact theParentController="{!this}" csName="CompSettings-0326"/>

<!-- ***** Client Household, page 1, order 2 ***** -->
<ExpECM:ExpClientHousehold theParentController="{!this}" csName="CompSettings-0327" suppressName="true"/>

<!-- ***** Contact, page 1, order 3 ***** -->
<ExpECM:ExpContact theParentController="{!this}" csName="CompSettings-0328"/>

<!-- ***** Case Record, page 1, order 4 ***** -->
<ExpECM:ExpCaseRecord theParentController="{!this}" csName="CompSettings-0329"/>

<!-- ***** Case Record, page 1, order 5 ***** -->
<ExpECM:ExpCaseRecord theParentController="{!this}" csName="CompSettings-0330"/>

<!-- ***** Case Record, page 1, order 6 ***** -->
<ExpECM:ExpCaseRecord theParentController="{!this}" csName="CompSettings-0331"/>

<!-- ***** Case Record, page 1, order 7 ***** -->
<ExpECM:ExpCaseRecord theParentController="{!this}" csName="CompSettings-0332"/>

<!-- ***** Case Record, page 1, order 8 ***** -->
<ExpECM:ExpCaseRecord theParentController="{!this}" csName="CompSettings-0333"/>

<!-- ***** Case Record, page 1, order 9 ***** -->
<ExpECM:ExpCaseRecord theParentController="{!this}" csName="CompSettings-0334"/>

<!-- ***** Household Members, page 1, order 10 ***** -->
<ExpECM:ExpHouseholdMembers theParentController="{!this}" csName="CompSettings-0335"/>

<!-- ***** Contact, page 1, order 11 ***** -->
<ExpECM:ExpContact theParentController="{!this}" csName="CompSettings-0336"/>

<!-- ***** Child Object List, page 1, order 12 ***** -->
<ExpECM:ExpChildObjectList theParentController="{!this}" csName="CompSettings-0337"/>

<!-- ***** Case Record, page 1, order 13 ***** -->
<ExpECM:ExpCaseRecord theParentController="{!this}" csName="CompSettings-0338"/>

<apex:pageBlockButtons >
<ExpECM:ExpNavigationButton id="theButtons" theParentController="{!this}" thePageNum="1" postSaveRerender="theForm"/>
</apex:pageBlockButtons>
</apex:pageBlock>
</apex:form>
<apex:includeScript value="{!$Resource.ExpECM__ExpJavaScript}"/>
</div>
</apex:page>